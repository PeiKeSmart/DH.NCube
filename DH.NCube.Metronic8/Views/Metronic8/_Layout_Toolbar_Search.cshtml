@using NewLife.Cube.Extensions
@{
    var set = ViewBag.PageSetting as PageSetting ?? PageSetting.Global;
}
<div class="d-inline-flex input-group w-lg-150px">
    @if (set.EnableKey)
    {
        <input name="q" type="search" id="q" value="@Context.Request.GetRequestValue("q")" class="form-control form-control-sm" placeholder="搜索关键字" />
    }
</div>